{
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "live-server": "^1.2.2",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1"
  },
  "scripts": {
    "tailwind": "tailwindcss  -i ./src/input.css -o build/tailwind.css --minify  & live-server  ",
    "dev": " live-server  & postcss src/input.css -o ./build/tailwind.css --minify --watch  ",
    "start": "live-server ./ ",
    "build": "postcss src/input.css -o build/tailwind.css --minify"
  }
}
